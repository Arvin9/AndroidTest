                <action android:name="com.talkingdata.sdk.TDAntiCheatingService" />
            </intent-filter>
        </service>
        <meta-data android:name="TD_APP_ID" android:value="C393A948E6F9496D8367AB4159D2FB02" />
        <meta-data android:name="TD_CHANNEL_ID" android:value="1" />
